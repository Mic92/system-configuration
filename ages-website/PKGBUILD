pkgname=ages-website
pkgver=20160301
pkgrel=1
pkgdesc='Contents for AGeS website'
arch=('any')
url='https://www.ages-skripte.org'
license=('custom:none')
depends=()
source=('ages-website.tar')
md5sums=('SKIP')

package() {
    cd "${srcdir}"
    install -d -m 0644 "${pkgdir}/usr/share/ages-website/"
    cp -R ages-website/ "${pkgdir}/usr/share/"
}
